  </configSections>
  <connectionStrings>   
    <add name="WebMakerDBEntities" connectionString="metadata=res://*/Models.CMSModel.csdl|res://*/Models.CMSModel.ssdl|res://*/Models.CMSModel.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=.\SQLEXPRESS;initial catalog=WebMakerDB;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />


    <add key="DefaultHost" value="www.ai-sportthings.com" />
    <!-- 
    <add key="DefaultHost" value="cms.xnet.world" />    
    <add key="DefaultHost" value="www.eightgeman.com" />
    <add key="DefaultHost" value="www.rollersports.org.tw" />
